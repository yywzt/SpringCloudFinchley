<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yw.task.mapper.UserTaskRecordMapper">
    <insert id="batchInsert">
        INSERT INTO `user_task_record`(`user_id`, `task_id`, `level`,`finished_date`)
        VALUES
        <foreach collection="userTaskRecords" item="userTaskRecord" separator="," open="(" close=")">
            #{userTaskRecord.userId},#{userTaskRecord.taskId},#{userTaskRecord.level},#{userTaskRecord.finishedDate}
        </foreach>
    </insert>
</mapper>